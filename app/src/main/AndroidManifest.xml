<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.example.kushagra.udaipur">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main2Activity"
            android:label="History of Udaipur" />
        <activity
            android:name=".geo_activity"
            android:label="Geography Of Udaipur" />
        <activity
            android:name=".eco_activity"
            android:label="Economy Of Rajasthan" />
        <activity
            android:name=".abt_activity"
            android:label="About Developer" />
        <activity android:name=".wiki_activity" android:label="Web View Activity(Wikipedia)"></activity>
    </application>

</manifest>